#include <stdio.h>


int main(void) {

	int i=0,j=0;

	char a[]=" ";
	printf("Press any alphabet, once you are done with your inputs\n");
	printf("Input : \n");
	while(1)
    {

        a[i]=getchar();
        if(a[i]>="a"&& a[i]<="z"||a[i]>="A" && a[i]<="Z")
           {
             break;
           }
            i++;
    }


	for(j=0;a[j]!=42;j++)
	{
		printf("%d\n",a[j]);
		printf("\n");
	}


	return 0;
}
